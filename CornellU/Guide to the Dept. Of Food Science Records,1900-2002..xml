			<unittitle label="Title:" encodinganalog="MARC 245">Dept. of Food Science records,
					<unitdate encodinganalog="MARC 245" type="inclusive">1900-2002.</unitdate>
			</unittitle>
			<unitid label="Collection Number:">21-26-3893</unitid>
