   <archdesc level="collection">
      <did>
         <head>DESCRIPTIVE SUMMARY</head>
         <unittitle label="Title:">Southern Tier Central Planning and Development Board Reports, 

        <unitdate type="inclusive">1968-1990</unitdate>

      
         </unittitle>
         <unitid label="Collection Number:">4476</unitid>
