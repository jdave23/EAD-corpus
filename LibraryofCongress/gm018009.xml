         <unitid label="Call No." encodinganalog="050" countrycode="US" repositorycode="US-DLC">G7821.S7 coll Farrell</unitid>
         <physdesc label="Extent" encodinganalog="300">21 maps : some colored ; various sizes </physdesc>
         <physdesc label="Extent" encodinganalog="300">1 folder </physdesc>
         <langmaterial label="Language">
            <language langcode="eng" encodinganalog="041">English</language>
            <descriptivenote>
               <p>Collection material in English </p>
            </descriptivenote>
         </langmaterial>
         <repository label="Location" encodinganalog="852">
            <corpname>
