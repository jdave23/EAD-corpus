      <c01 level="series">
        <did>
          <unitid>Series I</unitid>
          <unittitle encodinganalog="245$a">Genealogical </unittitle>
          <unitdate era="ce" calendar="gregorian" encodinganalog="245$f" normal="1819/1938">1819-1938 </unitdate>
          <physdesc>
