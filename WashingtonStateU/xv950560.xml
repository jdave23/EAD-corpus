	 	<c01 level="series">
	 		<did>
	 			<unittitle encodinganalog="title">12/1-2: D.J. Crowley, Superintendent, WSU
	 				Coastal Washington Research and Extension Unit, Long Beach, WA, April
	 				17, 1973.</unittitle>
	 		</did>
	 		<c02 level="file">
	 			<did>
	 				<container type="tape">1, Side A</container>
	 				<container type="time">Minutes (approx.): 0 - 3</container>
	 				<unittitle encodinganalog="title">College experiences in the
	 					1920's.</unittitle>
	 			</did>
	 		</c02>
	 		<c02 level="file">
	 			<did>
	 				<container type="tape">1, Side A</container>
	 				<container type="time">Minutes (approx.): 3 - 13</container>
	 				<unittitle encodinganalog="title">Growing blueberries. Work on cranberry
	 					disease. Problems growers had when he started work at The Coastal
	 					Washington Research and Extension Unit. Fruitworms.</unittitle>
	 			</did>
	 		</c02>
	 		<c02 level="file">
	 			<did>
	 				<container type="tape">1, Side A</container>
	 				<container type="time">Minutes (approx.): 13 - 25</container>
	 				<unittitle encodinganalog="title">Insecticides that were used. Insect
	 					problems, Specifics on insecticide toxicity. Smudge
	 					pots.</unittitle>
	 			</did>
	 		</c02>
	 		<c02 level="file">
	 			<did>
	 				<container type="tape">1, Side B</container>
	 				<container type="time">Minutes (approx.): 0 - 12</container>
	 				<unittitle encodinganalog="title">Agricultural situation during World
	 					War II, Japanese farmers,More on disease and crop problems.
	 					Frost.</unittitle>
	 			</did>
	 		</c02>
	 		<c02 level="file">
	 			<did>
	 				<container type="tape">1, Side B</container>
	 				<container type="time">Minutes (approx.): 12 - 30</container>
	 				<unittitle encodinganalog="title">Spraying. More about frost problems.
	 					His experiments in controlling frost in cranberry plots. Problems
	 					getting funds for frost control experiments.</unittitle>
	 			</did>
	 		</c02>
	 		<c02 level="file">
	 			<did>
	 				<container type="tape">2, Side A</container>
	 				<container type="time">Minutes (approx.): 0 - 7</container>
	 				<unittitle encodinganalog="title">Controlling frost by sprinkling. His
	 					problems getting funding for experiments.</unittitle>
	 			</did>
	 		</c02>
	 		<c02 level="file">
	 			<did>
	 				<container type="tape">2, Side A</container>
	 				<container type="time">Minutes (approx.): 7 - 14</container>
	 				<unittitle encodinganalog="title">His relationship with Wilson Compton.
	 					Cranberry crop. Installing a sprinkling system to fight
	 					frost.</unittitle>
	 			</did>
	 		</c02>
	 		<c02 level="file">
	 			<did>
	 				<container type="tape">2, Side A</container>
	 				<container type="time">Minutes (approx.): 14 - 22</container>
	 				<unittitle encodinganalog="title">Success of the sprinkling system. More
	 					on the installment.</unittitle>
	 			</did>
	 		</c02>
	 		<c02 level="file">
	 			<did>
	 				<container type="tape">2, Side A</container>
	 				<container type="time">Minutes (approx.): 22 - 30</container>
	 				<unittitle encodinganalog="title">Saving frost-bitten spring wheat.
	 					Biblical analogy.</unittitle>
	 			</did>
	 		</c02>
	 		<c02 level="file">
	 			<did>
	 				<container type="tape">2, Side B</container>
	 				<container type="time">Minutes (approx.): 0 - 3</container>
	 				<unittitle encodinganalog="title">Thoughts about learning and
	 					experimentation. The changes brought on by the sprinkling
	 					system.</unittitle>
	 			</did>
	 		</c02>
	 		<c02 level="file">
	 			<did>
	 				<container type="tape">2, Side B</container>
	 				<container type="time">Minutes (approx.): 3 - 12</container>
	 				<unittitle encodinganalog="title">Care of the cranberry crop. Use of
	 					herbicides and fertilizers.</unittitle>
	 			</did>
	 		</c02>
	 		<c02 level="file">
	 			<did>
	 				<container type="tape">2, Side B</container>
	 				<container type="time">Minutes (approx.): 12 - 16</container>
	 				<unittitle encodinganalog="title">Talks about some of his fellow
	 					workers. Changes in agricultural methods.</unittitle>
	 			</did>
	 		</c02>
